{
  "compilerOptions": {
    /* Core Paths */
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    /* Language and Module Settings */
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    /* Strictness and Compatibility */
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    /* Type Checking Performance */
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "src",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
, "proxy.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
